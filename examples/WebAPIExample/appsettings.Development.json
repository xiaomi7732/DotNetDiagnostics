{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "DotNet.Diagnostics.Counters": "Debug"
    }
  },
  "DotNetCounters": {
    "InvokingSecret": "1111",
    "CustomEventCounters": {
      "ClearDefaultEventCounters": false,
      "Items": [
        {
          "Name": "System.Runtime",
          "EventLevel": "Verbose",
          "Keywords": "0xffffffff",
          "IntervalInSeconds": 1,
          "Filters": [
            "cpu-usage",
            "active-timer-count",
            "working-set"
          ]
        }
      ]
    },
    "Sinks": {
      "AzureBlob": {
        "ServiceUri": "https://dotnetdiagsabsink.blob.core.windows.net/"
      },
      "LocalFile": {
        "OutputFolder": "c:\\dotnet-counters"
      }
    },
    "Triggers": {
      "ProcessStart": {
        "IsEnabled": true,
        "InitialDelay": "00:00:10"
      }
    }
  },
  "ApplicationInsights": {
    "ConnectionString": "Copy-it-from-your-application-insights-resource"
  }
}